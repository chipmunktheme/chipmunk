{
  "name": "chipmunk-theme/chipmunk-theme",
  "homepage": "https://chipmunktheme.com",
  "description": "Chipmunk WordPress Theme makes it ridiculously easy to make a content curation website.",
  "authors": [
    {
      "name": "Made by Less",
      "email": "hello@madebyless.com",
      "homepage": "https://madebyless.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Chipmunk\\": "src",
      "App\\": "app"
    },
    "files": [
      "vendor/madebyless/merlin-wp/class-merlin.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/library/"
    }
  ],
  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "5.*",
    "log1x/acf-composer": "^2.0",
    "log1x/sage-directives": "^1.1",
    "madebyless/lessi": "^1.0",
    "madebyless/merlin-wp": "dev-master",
    "roots/acorn": "^2.1",
    "timber/timber": "^2.0.0-alpha.6"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true,
      "pivvenit/acf-pro-installer": true
    }
  },
  "scripts": {
    "post-autoload-dump": "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
  }
}
