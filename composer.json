{
  "name": "chipmunk-theme/chipmunk-theme",
  "homepage": "https://chipmunktheme.com/",
  "description": "A WordPress Theme for Content Curation",
  "authors": [
    {
      "name": "Made by Less",
      "email": "hello@madebyless.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Chipmunk\\": "includes"
    },
    "files": [
      "vendor/chipmunk-theme/chipmunk-wizard/class-almond.php"
    ]
  },
  "repositories": [
    {
        "type":"composer",
        "url": "https://connect.advancedcustomfields.com"
    },
    {
        "type": "path",
        "url": "../chipmunk-wizard",
        "options": {
            "symlink": true
        }
    }
  ],
  "require": {
    "chipmunk-theme/chipmunk-wizard": "@dev",
    "wpengine/advanced-custom-fields-pro": "^6.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
