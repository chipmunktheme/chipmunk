{
  "version": 2,
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "custom": {
      "typography": {
        "scale": 1.25,
        "line-height": 1.618,
        "small": "1.6rem",
        "normal": "calc(var(--wp--custom--typography--small) * var(--wp--custom--typography--scale))",
        "medium": "calc(var(--wp--custom--typography--normal) / var(--wp--custom--typography--scale))",
        "large": "calc(var(--wp--custom--typography--medium) * var(--wp--custom--typography--scale))",
        "xlarge": "calc(var(--wp--custom--typography--large) * var(--wp--custom--typography--scale))",
        "huge": "calc(var(--wp--custom--typography--xlarge) * var(--wp--custom--typography--scale))"
      }
    },
    "typography": {
      "dropCap": false,
      "customFontSize": false,
      "fontFamilies": [
        {
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "body"
        }
      ],
      "fontSizes": [
        {
          "name": "Small",
          "size": "clamp(var(--wp--custom--typography--normal), calc(1rem + var(--wp--custom--typography--large--preferred, 3vw)), var(--wp--custom--typography--large))",
          "slug": "small"
        },
        {
          "name": "Normal",
          "size": "clamp(var(--wp--custom--typography--small), calc(1rem + var(--wp--custom--typography--normal--preferred, 1vw)), var(--wp--custom--typography--normal))",
          "slug": "normal"
        },
        {
          "name": "Medium",
          "size": "clamp(var(--wp--custom--typography--normal), calc(1rem + var(--wp--custom--typography--medium--preferred, 2vw)), var(--wp--custom--typography--medium))",
          "slug": "large"
        },
        {
          "name": "Large",
          "size": "clamp(var(--wp--custom--typography--medium), calc(1rem + var(--wp--custom--typography--large--preferred, 3vw)), var(--wp--custom--typography--large))",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "size": "clamp(var(--wp--custom--typography--large), calc(1rem + var(--wp--custom--typography--xlarge--preferred, 4vw)), var(--wp--custom--typography--xlarge))",
          "slug": "xlarge"
        },
        {
          "name": "Huge",
          "size": "clamp(var(--wp--custom--typography--xlarge), calc(1rem + var(--wp--custom--typography--huge--preferred, 5vw)), var(--wp--custom--typography--huge))",
          "slug": "huge"
        }
      ]
    }
  }
}
